{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**.scss"
, "src/ProtectedRoute.tsx", "src/GoogleAnalitycs.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
